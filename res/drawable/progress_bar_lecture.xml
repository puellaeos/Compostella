<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:id="@android:id/background">
	<shape>
	   <corners android:radius="5dip" />
	   <gradient
		android:startColor="@color/Bleu1"
		android:centerColor="@color/Bleu2"
		android:centerY="0.50"
		android:endColor="@color/Bleu1"
		android:angle="270" />
	</shape>
  </item>
  <item android:id="@android:id/secondaryProgress">
	<clip>
	   <shape>
		<corners android:radius="5dip" />
		<gradient
		    android:startColor="@color/Orange1"
			android:centerColor="@color/Orange2"
			android:centerY="0.50"
			android:endColor="@color/Orange1"
			android:angle="90" />
	   </shape>
	</clip>
  </item>
  <item android:id="@android:id/progress">
	<clip>
	   <shape>
		<corners android:radius="5dip" />
		<gradient
		    android:startColor="@color/Bleu1"
			android:centerColor="@color/Bleu2"
			android:centerY="0.50"
			android:endColor="@color/Bleu1"
			android:angle="90" />
	   </shape>
	</clip>
  </item>
</layer-list>